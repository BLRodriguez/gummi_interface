<!-- -*- mode: XML -*- -->

<launch>
    <node name="dynamixel_manager_arm" pkg="dynamixel_controllers" type="controller_manager.py" required="true" output="screen">
        <rosparam>
            namespace: dxl_manager
            serial_ports:
                gummi_port_arm:
                    port_name: "/dev/ttyUSB3"
                    baud_rate: 1000000
                    min_motor_id: 1
                    max_motor_id: 25
                    update_rate: 200
        </rosparam>
    </node>
</launch>
